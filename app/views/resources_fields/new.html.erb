<h1>New Resources Field</h1>

<% form_for([@resources_table, @resources_field]) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :resources_table_id %><br />
    <%= f.text_field :resources_table_id %>
  </p>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', resources_table_resources_fields_path %>
