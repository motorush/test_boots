<h1>Editing Resources Field</h1>

<% form_for([@resources_table, @resources_field]) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :resources_table_id %><br />
    <%= f.text_field :resources_table_id %>
  </p>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', resources_table_resources_field_path(@resources_table, @resources_field) %> |
<%= link_to 'Back', resources_table_resources_fields_path(@resources_table) %>
