<h1>Listing resources_datas</h1>

<table>
  <tr>
    <th>Value</th>
  </tr>

<% @resources_datas.each do |resources_data| %>
  <tr>
    <td><%=h resources_data.value %></td>
    <td><%= link_to 'Show', resources_data %></td>
    <td><%= link_to 'Edit', edit_resources_data_path(resources_data) %></td>
    <td><%= link_to 'Destroy', resources_data, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New resources_data', new_resources_data_path %>